        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

if(gs.hasRole('admin') || gs.hasRole('x_1850913_facili_0.facility_manager')){
		return;
	}
var myGroups = gs.getUser().getMyGroups(); 
    
    var groupsStr = myGroups.toString();

    if (groupsStr) {
        var query = current.addQuery('assignment_group', 'IN', groupsStr);
        query.addOrCondition('opened_by', gs.getUserID());
        
    } else {
        current.addQuery('opened_by', gs.getUserID());
    }
	
})(current, previous);]]></script>
